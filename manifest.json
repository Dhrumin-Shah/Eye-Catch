{
  "name": "Eye Catch",
  "version": "1.0",
  "description": "Youtube eye movement extension",
  "permissions": ["activeTab","declarativeContent", "storage"],
  "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "page_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/EyecatchRed16.png",
        "32": "images/EyecatchRed48.png"
      }
    },
  "manifest_version": 2
}
